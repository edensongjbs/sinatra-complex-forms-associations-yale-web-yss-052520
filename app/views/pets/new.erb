<h1>Create a new Pet</h1>
<form method="POST" action="/pets">
<input type="text" id="pet_name" name="pet[name]"> 
<label>Select an Owner</label>
<% @owners.each do |owner| %>
<input type="radio" id="<%=owner.id%>" name="pet[owner_id]" value="<%=owner.id%>"><%=owner.name%></input>
<%end%>
<label>Or Create a new Owner</label>
<input type="text" id="owner_name" name="owner[name]">
<input type="submit" id="Create Pet" value="Create Pet">

